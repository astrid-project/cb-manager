{post_description}
---
summary: {post_summary}
description: {post_description}
parameters:
    - name: id
      required: true
      in: path
      type: String
    - name: payload
      required: true
      in: body
      schema:
        type: array
        items: {schema}
tags: [{tag}]
responses:
    200:
        description: {post_responses_200_description}
        schema:
            type: array
            items:
                oneOf:
                - ResponseSchema
                - ErrorSchema
    400:
        description: Bad request.
        schema: HTTPErrorSchema
    401:
        description: Unauthorized.
        schema: HTTPErrorSchema

