{
    "id": "winlogbeat",
    "parameters": [{
        "id": "system-enabled",
        "type": "boolean",
        "description": "Enable the collection of system log",
        "config": {
            "schema": "yaml",
            "source": "C:\\ProgramData\\Elastic\\Beats\\winlogbeat\\winlogbeat.yml",
            "path": [
                "winlogbeat.event_logs",
                "1",
                "enabled"
            ]
        }
    }],
    "actions": [{
            "id": "start",
            "config": {
                "cmd": "winlogbeat.exe -c winlogbeat.yml"
            }
        },
        {
            "id": "stop",
            "config": {
                "cmd": "sudo killall -9 cubebeat"
            }
        }
    ]
}